@model PagedList.IPagedList<SysDep>

@{
    ViewBag.Title = "部门管理";
}
<div>
    <ul class="breadcrumb admin-bread-ul">
        <li><a href="@Url.Action("Index","SysUser")">系统管理</a> <span class="divider">/</span></li>
        <li class="active">部门管理</li>
    </ul>
</div>
@foreach (SysRight r in ViewBag.pageTopRightList)
{
    if (r.ActionType == "View")
    {
        <a class="btn btn-primary" href="@r.MenuUrl" )>@r.DisplayName</a>
    }
    else
    {
        <a class="btn btn-primary ls-table-@r.ActionType.ToLower()" url="@r.MenuUrl" )>@r.DisplayName</a>
    }
}
@if (Model != null && Model.Count > 0)
{
    Html.RenderPartial("DepList_Pa");
}
else { Html.RenderPartial("_NoData"); }
